{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "babelOptions": {
        "presets": ["@babel/preset-react"]
      }
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
    "env": {
      "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": "off"
    }
}
